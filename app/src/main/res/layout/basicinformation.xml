             android:id="@+id/email"
             android:drawablePadding="10dp"
             android:layout_marginLeft="30dp"
             android:layout_width="wrap_content"
             android:layout_height="64px"
             android:drawableLeft="@mipmap/email"
             android:gravity="center_vertical"
